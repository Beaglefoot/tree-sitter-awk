{
  "name": "tree-sitter-awk",
  "version": "0.3.2",
  "author": "Beaglefoot <ch-e-st@yandex.ru>",
  "license": "MIT",
  "keywords": [
    "parser",
    "awk",
    "gawk"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Beaglefoot/tree-sitter-awk.git"
  },
  "scripts": {
    "build": "tree-sitter generate && node-gyp build",
    "build:wasm": "tree-sitter build-wasm",
    "types:watch": "node generate-types.js",
    "trim": "sed ' s/ \\[.*\\]//; s/\\w\\+:// '",
    "test": "tree-sitter test"
  },
  "dependencies": {
    "nan": "2.14.2"
  },
  "devDependencies": {
    "prettier": "2.3.2",
    "tree-sitter-cli": "0.19.2"
  },
  "main": "bindings/node",
  "tree-sitter": [
    {
      "scope": "source.awk",
      "file-types": [
        "awk",
        "gawk"
      ]
    }
  ]
}
